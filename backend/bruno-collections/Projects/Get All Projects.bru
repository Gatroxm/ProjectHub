meta {
  name: Get All Projects
  type: http
  seq: 2
}

get {
  url: {{baseUrl}}/projects
  body: none
  auth: bearer
}

auth:bearer {
  token: {{token}}
}

params:query {
  page: 1
  limit: 10
  status: active
}

tests {
  test("should return 200", function() {
    expect(res.getStatus()).to.equal(200);
  });
  
  test("should return projects array", function() {
    expect(res.getBody().projects).to.be.an('array');
  });
}